{
    "name": "表单助手",
    "version": "1.0",
    "description": "为页面表单创建快照以便之后自动完成，以及表单初次自动完成",
    "author": "弘树",
    "background": {
        "scripts": ["background.js"]
    },
    "page_action": {
        "default_icon": "icon.png",
        "default_title": "表单快照",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": ["jquery.min.js", "jquery.xpath.min.js", "content-script.js"],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "icons": {
        "48": "icon.png"
    },
    "permissions": [
        "storage"
    ],
    "manifest_version": 2,
    "web_accessible_resources": [
        "jquery-1.10.2.min.map"
    ]
}